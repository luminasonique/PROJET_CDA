{% extends 'base.html.twig' %}

{% block title %}UserInfo{% endblock %}

{% block body %}
    <h1>UserInfo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_info.id }}</td>
            </tr>
            <tr>
                <th>User_cv</th>
                <td>{{ user_info.userCv }}</td>
            </tr>
            <tr>
                <th>User_linkedin</th>
                <td>{{ user_info.userLinkedin }}</td>
            </tr>
            <tr>
                <th>User_git</th>
                <td>{{ user_info.userGit }}</td>
            </tr>
            <tr>
                <th>User_description</th>
                <td>{{ user_info.userDescription }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ user_info.createdAt ? user_info.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ user_info.updatedAt ? user_info.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_info_index') }}">back to list</a>

    <a href="{{ path('app_user_info_edit', {'id': user_info.id}) }}">edit</a>

    {{ include('user_info/_delete_form.html.twig') }}
