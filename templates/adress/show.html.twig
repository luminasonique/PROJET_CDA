{% extends 'base.html.twig' %}

{% block title %}Adress{% endblock %}

{% block body %}
    <h1>Adress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adress.id }}</td>
            </tr>
            <tr>
                <th>User_account_id</th>
                <td>{{ adress.userAccountId }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ adress.street }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ adress.city }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ adress.state }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ adress.code }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ adress.country }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ adress.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ adress.createdAt ? adress.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ adress.updatedAt ? adress.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_adress_index') }}">back to list</a>

    <a href="{{ path('app_adress_edit', {'id': adress.id}) }}">edit</a>

    {{ include('adress/_delete_form.html.twig') }}
{% endblock %}
