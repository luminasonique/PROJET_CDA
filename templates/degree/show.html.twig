{% extends 'base.html.twig' %}

{% block title %}Degree{% endblock %}

{% block body %}
    <h1>Degree</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ degree.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ degree.name }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ degree.level }}</td>
            </tr>
            <tr>
                <th>Speciality</th>
                <td>{{ degree.speciality }}</td>
            </tr>
            <tr>
                <th>Obtained_at</th>
                <td>{{ degree.obtainedAt ? degree.obtainedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ degree.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ degree.createdAt ? degree.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ degree.updatedAt ? degree.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_degree_index') }}">back to list</a>

    <a href="{{ path('app_degree_edit', {'id': degree.id}) }}">edit</a>

    {{ include('degree/_delete_form.html.twig') }}
{% endblock %}
