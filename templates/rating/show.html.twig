{% extends 'base.html.twig' %}

{% block title %}Rating{% endblock %}

{% block body %}
    <h1>Rating</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rating.id }}</td>
            </tr>
            <tr>
                <th>Supervisor_id</th>
                <td>{{ rating.supervisorId }}</td>
            </tr>
            <tr>
                <th>Module_id</th>
                <td>{{ rating.moduleId }}</td>
            </tr>
            <tr>
                <th>User_rating_id</th>
                <td>{{ rating.userRatingId }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ rating.score }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ rating.comment }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ rating.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ rating.createdAt ? rating.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ rating.updatedAt ? rating.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_rating_index') }}">back to list</a>

    <a href="{{ path('app_rating_edit', {'id': rating.id}) }}">edit</a>

    {{ include('rating/_delete_form.html.twig') }}
{% endblock %}
